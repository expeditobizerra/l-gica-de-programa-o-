algoritmo "antecessor_e_sucessor"
var
N1, antecessor, sucessor : inteiro
inicio
         escreva("digite um número: ")
         leia(N1)
         antecessor <- N1 - 1
         escreval("o antecessor é: " , antecessor)
         sucessor <- N1 +1
         escreval("o sucessor é: ", sucessor)

fimalgoritmo