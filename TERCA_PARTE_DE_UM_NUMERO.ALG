algoritmo "tercaparte_de_um_numero"
var
N1, DOBRO , tercaparte : REAL
inicio
         escreva("digite um número: ")
         leia(N1)
         DOBRO <- N1 * 2
         escreval("O dobro de:" , N1 , " é" , DOBRO)
         tercaparte <- N1 / 3
         escreval("A terça parte de:" , N1 , " é" , tercaparte:5:2)
fimalgoritmo